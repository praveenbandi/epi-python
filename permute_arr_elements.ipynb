   "source": [
    "from typing import List"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "def permute(perm: List[int], A: List[int]) -> None:\n",
    "    result = [0] * len(perm)\n",
    "    for i, x in enumerate(perm):\n",
    "        result[x] = A[i]\n",
    "    print(result)"
   ]
  },
